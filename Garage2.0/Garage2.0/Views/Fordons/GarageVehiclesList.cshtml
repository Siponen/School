@using Garage2._0.Models;
@model IEnumerable<Garage2._0.Models.Fordon>

@{
    ViewBag.Title = "List of Vehicles in garage";
}

<h2>List of Vehicles in garage</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegNr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Typ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Färg)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Märke)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IncheckDatum)
        </th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegNr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Typ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Färg)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Märke)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IncheckDatum)
        </td>
    </tr>
}

</table>
